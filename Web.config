      <add name="JavaScriptHandler" verb="*" path="*.js" preCondition="integratedMode" type="WebOptimizer.Handlers.JavaScriptHandler, WebOptimizer"/>
      <add name="StylesheetHandler" verb="*" path="*.css" preCondition="integratedMode" type="WebOptimizer.Handlers.StylesheetHandler, WebOptimizer"/>
    </handlers>
